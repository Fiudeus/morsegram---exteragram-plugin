def is_morse(text: str) -> bool:
    s = text.strip()
    if not s:
        return False

    # Treat as Morse only if it's in our encoded format: marker + WORD_SEP + payload
    if WORD_SEP in s:
        first = s.split(WORD_SEP, 1)[0].strip()
        if first in (MORSE_RU_MARK, MORSE_EN_MARK):
            return True

    return False
